{
  "name": "sandbox-feathers-server",
  "description": "Sandbox Feathers",
  "version": "0.0.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Goran",
    "email": "gospaso@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": "^10.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "start": "node src/",
    "dev": "nodemon src/",
    "db:drop": "sequelize db:drop",
    "db:create": "sequelize db:create",
    "db:reset": "npm run db:drop && npm run db:create",
    "db:migrate": "sequelize db:migrate",
    "fakes": "cross-env NODE_ENV=fakes feathers-plus g fakes",
    "seed": "cross-env PORT=3333 node src/ --seed",
    "test": "cross-env NODE_ENV=test npm run eslint && npm run test:coverage",
    "test:dev": "cross-env NODE_ENV=test npm run mocha -- --watch --reporter dot",
    "test:coverage": "cross-env NODE_ENV=test nyc --reporter=html npm test",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "mocha": "mocha test/ --recursive --exit --timeout 10000",
    "test:all": "yarn run eslint && cross-env NODE_ENV=development npm run mocha",
    "start:seed": "cross-env NODE_ENV=development node src/ --seed"
  },
  "dependencies": {
    "@feathers-plus/graphql": "^1.10.0",
    "@feathers-plus/test-utils": "^0.3.5",
    "@feathersjs/authentication": "^2.1.7",
    "@feathersjs/authentication-jwt": "^2.0.1",
    "@feathersjs/authentication-local": "^1.2.1",
    "@feathersjs/authentication-oauth2": "^1.1.0",
    "@feathersjs/configuration": "^2.0.0",
    "@feathersjs/errors": "^3.3.0",
    "@feathersjs/express": "^1.2.3",
    "@feathersjs/feathers": "^3.1.7",
    "@feathersjs/socketio": "^3.2.2",
    "ajv": "5.5.2",
    "cloudinary": "^1.13.2",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "feathers-authentication-hooks": "^0.3.1",
    "feathers-authentication-management": "^2.0.1",
    "feathers-hooks-common": "^4.16.3",
    "feathers-mailer": "^3.0.1",
    "feathers-permissions": "^0.2.1",
    "feathers-sequelize": "^3.1.2",
    "feathers-swagger": "^0.7.2",
    "graphql-resolvers-ast": "^1.4.0",
    "helmet": "^3.13.0",
    "lodash.merge": "^4.6.1",
    "merge-graphql-schemas": "^1.5.3",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "pg": "^7.8.0",
    "pug": "^2.0.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "sequelize": "^4.38.0",
    "serve-favicon": "^2.5.0",
    "uuid": "^3.3.2",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.7",
    "push-dir": "^0.4.1",
    "sequelize-cli": "^5.4.0"
  }
}
